{
  "name": "@pderas/tabular",
  "version": "1.1.0",
  "description": "A new vue table component from pderas",
  "main": "dist/tables-and-chairs.umd.min.js",
  "author": "Reed Jones <reed.jones@pderas.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "lint": "eslint src --ext .js,.vue",
    "lint:fix": "eslint src --ext .js,.vue --fix",
    "serve": "vue-cli-service serve ./public/example.js",
    "watch": "vue-cli-service build --watch --target lib --name tables-and-chairs ./src/index.js",
    "bundle": "vue-cli-service build --target lib --name tables-and-chairs ./src/index.js",
    "build": "npm run lint && npm run bundle"
  },
  "keywords": [
    "vue-table",
    "tabular",
    "pderas",
    "table",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:PDERAS/tabular.git"
  },
  "bugs": {
    "url": "https://github.com/PDERAS/tabular/issues"
  },
  "dependencies": {
    "@pderas/dragon-drop": "^1.2.0"
  },
  "devDependencies": {
    "@pderas/eslint-config": "^1.1.1",
    "@vue/babel-preset-app": "^4.2.3",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "eslint": "^6.8.0",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
